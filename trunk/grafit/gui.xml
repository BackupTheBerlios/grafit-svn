<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Resource>
  <Window name="'mainwin'" title="'mingui.manual'" size="(640, 480)">
    <Menubar>
      <Menu label="'&amp;File'">
        <MenuItem command="'file-new'"/>
        <MenuItem command="'file-open'"/>
        <MenuItem command="'file-save'"/>
        <Separator/>
        <MenuItem command="'file-exit'"/>
      </Menu>
    </Menubar>
    <Box orientation="'vertical'">
      <Splitter orientation="'horizontal'">
        <Panel position="'left'" _width="100">
          <Tree columns="['Topics']" _label="'Topics'" _image="'open'" name="'tree'"/>
        </Panel>
        <Splitter orientation="'vertical'">
          <Notebook _stretch="1.">
            <Html name="'html'" _label="'text'"/>
            <PythonEditor name="'editor'" _label="'code'"/>
            <Box _label="'demo'">
              <Button _expand="False" _stretch="0" text="'button1'" toggle="True"/>
              <Button _expand="False" _stretch="0" text="'button'"/>
            </Box>
          </Notebook>
          <Panel name="'xpanel'" _width="100" position="'bottom'">
            <PythonShell _label="'Command line'" _image="'console'"/>
          </Panel>
        </Splitter>
      </Splitter>
    </Box>
  </Window>
  <Commands name="'foo'">
    <Command label="'file-new'" image="'file-new'" desc="'Create a new file'"/>
  </Commands>
  <Images>
    <Image label="'file=new'" path="''"/>
    <DirImageProvider path="'data/images'"/>
  </Images>
</Resource>
